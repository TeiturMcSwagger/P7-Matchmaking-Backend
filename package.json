{
  "name": "p7-matchmaking",
  "version": "1.0.0",
  "description": "uni project",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:tsoa && tsc",
    "clean": "rm -rf ./dist && rm -rf ./build && mkdir build",
    "build:tsoa": "npm run clean && npm run build:routes && npm run build:swagger",
    "build:swagger": "tsoa swagger",
    "build:routes": "tsoa routes",
    "dev": "npm run build:tsoa && nodemon ./lib/server.ts | pino-pretty",
    "withstatics": "cp ./build/swagger -r ./dist/build/ && cp ./lib/gamelist.json ./dist/lib/",
    "start": "npm run withswagger && node ./dist/lib/server.js",
    "restartprod": "pm2 restart all",
    "startdev": "npm run withstatics && pm2 start ./dist/lib/server.js -i 0 --name P7-Matchmaking",
    "prod": "npm run build && npm run startdev",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/randomstring": "^1.1.6",
    "body-parser": "^1.18.3",
    "discord.js": "^11.4.2",
    "dotenv": "^6.1.0",
    "pm2": "^2.4.2",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^3.2.0",
    "inversify-express-utils": "^6.2.0",
    "mongoose": "^5.3.2",
    "morgan": "^1.9.1",
    "npm": "^6.4.1",
    "pino": "^5.8.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.12",
    "swagger-ui-dist": "^3.19.4",
    "swagger-ui-express": "^4.0.1",
    "tsoa": "^2.2.5"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.2.19",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/pino": "^5.20.0",
    "@types/randomstring": "^1.1.6",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "pino-pretty": "^2.2.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
}
